<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="1000.0" maxWidth="1500.0" minHeight="1000.0" minWidth="1500.0" prefHeight="1000.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view.ImportExportMenu">
   <children>
      <BorderPane prefHeight="1000.0" prefWidth="1500.0" styleClass="importExportMenu" stylesheets="@../CSSFiles/Menus.css">
         <right>
            <ScrollPane fx:id="cardsList" opacity="0.3" prefHeight="800.0" prefWidth="500.0" BorderPane.alignment="CENTER">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
              </content>
               <BorderPane.margin>
                  <Insets right="120.0" />
               </BorderPane.margin>
            </ScrollPane>
         </right>
         <top>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="250.0" BorderPane.alignment="CENTER">
               <children>
                  <Label alignment="CENTER" prefHeight="50.0" prefWidth="500.0" styleClass="backYellow" text="Import Card" onMouseClicked="#importCards">
                     <font>
                        <Font size="30.0" />
                     </font></Label>
                  <Label alignment="CENTER" prefHeight="50.0" prefWidth="500.0" styleClass="backYellow" stylesheets="@../CSSFiles/Menus.css" text="Export Card" textFill="#34495e" onMouseClicked="#exportCards">
                     <font>
                        <Font size="30.0" />
                     </font></Label>
               </children></HBox>
         </top>
         <bottom>
            <HBox prefHeight="220.0" prefWidth="1500.0" BorderPane.alignment="CENTER" />
         </bottom>
         <center>
            <Separator orientation="VERTICAL" prefHeight="200.0" prefWidth="18.0" BorderPane.alignment="CENTER" />
         </center>
         <left>
            <Pane fx:id="importPane" prefHeight="800.0" prefWidth="500.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox fx:id="importPlace" alignment="TOP_CENTER" layoutX="100.0" layoutY="67.0" opacity="0.2" prefHeight="380.0" prefWidth="300.0" spacing="20.0" styleClass="importVBox" stylesheets="@../CSSFiles/Menus.css" />
                  <Label alignment="CENTER" layoutX="125.0" layoutY="14.0" prefHeight="40.0" prefWidth="250.0" text="Drag &amp; Drop Here :" textFill="#daf7a6">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <ScrollPane fx:id="cardsPreShow" layoutX="99.0" layoutY="466.0" prefHeight="200.0" prefWidth="300.0" visible="false">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
                    </content>
                  </ScrollPane>
               </children>
               <BorderPane.margin>
                  <Insets left="120.0" />
               </BorderPane.margin></Pane>
         </left></BorderPane>
   </children>
</AnchorPane>
