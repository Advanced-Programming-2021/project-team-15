<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="1000.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view.DeckEdit">
   <children>
      <Pane layoutX="84.0" layoutY="52.0" prefHeight="750.0" prefWidth="1354.0">
         <children>
            <HBox prefHeight="707.0" prefWidth="1319.0">
               <children>
                  <ScrollPane fx:id="trunk" prefHeight="707.0" prefWidth="493.0">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
                    </content>
                  </ScrollPane>
                  <ScrollPane fx:id="mainDeck" prefHeight="707.0" prefWidth="441.0">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
                    </content>
                  </ScrollPane>
                  <VBox alignment="CENTER" prefHeight="707.0" prefWidth="412.0">
                     <children>
                        <ScrollPane fx:id="sideDeck" prefHeight="200.0" prefWidth="390.0">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
                          </content>
                        </ScrollPane>
                        <ToolBar prefHeight="70.0" prefWidth="408.0">
                          <items>
                              <Button mnemonicParsing="false" prefHeight="43.0" prefWidth="139.0" text="add to main deck" onMouseClicked="#addCardToMain">
                                  <font>
                                      <Font size="15.0" />
                                  </font>
                              </Button>
                              <Button mnemonicParsing="false" prefHeight="42.0" prefWidth="130.0" text="add to side deck" onMouseClicked="#addCardToSide">
                                  <font>
                                      <Font size="15.0" />
                                  </font>
                              </Button>
                              <Button mnemonicParsing="false" prefHeight="39.0" prefWidth="107.0" text="remove" onMouseClicked="#removeCard">
                                  <font>
                                      <Font size="15.0" />
                                  </font>
                              </Button>
                          </items>
                        </ToolBar>
                        <HBox prefHeight="346.0" prefWidth="390.0">
                           <children>
                              <Pane prefHeight="266.0" prefWidth="213.0" />
                              <ImageView fx:id="chosenCard" fitHeight="307" fitWidth="217.5" pickOnBounds="true" preserveRatio="true" />
                              <Pane prefHeight="200.0" prefWidth="200.0" />
                           </children>
                        </HBox>
                        <Label alignment="CENTER" prefHeight="122.0" prefWidth="393.0" text="jj">
                            <font>
                                <Font size="25.0" />
                            </font>
                        </Label>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
