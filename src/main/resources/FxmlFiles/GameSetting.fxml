<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="settingBack" stylesheets="@../CSSFiles/Duel.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view.GameSettingController">
   <children>
      <Label alignment="CENTER" layoutX="233.0" layoutY="14.0" prefHeight="27.0" prefWidth="134.0" text="Setting" textFill="#5d151b">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <VBox alignment="TOP_CENTER" layoutX="14.0" layoutY="49.0" prefHeight="302.0" prefWidth="230.0" spacing="10.0" styleClass="settingContainer" stylesheets="@../CSSFiles/Duel.css">
