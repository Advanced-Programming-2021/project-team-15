            <HBox alignment="CENTER" prefHeight="46.0" prefWidth="212.0" spacing="5.0">
               <children>
                  <Label text="Volume : " textFill="#5d151b">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Slider prefHeight="35.0" prefWidth="140.0">
                     <cursor>
                        <Cursor fx:constant="MOVE" />
                     </cursor></Slider>
               </children>
            </HBox>
            <Separator prefWidth="200.0" />
